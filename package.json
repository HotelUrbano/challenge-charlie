{
  "name": "react_webpack",
  "version": "1.0.0",
  "description": "Weather App",
  "main": "index.js",
  "scripts": {
    "setup": "npm i; export NVM_DIR=$HOME/.nvm && source $NVM_DIR/nvm.sh; nvm use",
    "build": "webpack --mode production",
    "start": "cp config/config.dev.js config/index.js; cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.common.js --hot",
    "dev": "cp config/config.dev.js config/index.js; npx webpack-dev-server --host 0.0.0.0 --mode development --config webpack/webpack.common.js --hot",
    "lint": "eslint --fix .",
    "lint-check": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:build": "docker build . -t weatherapp",
    "docker:run": "docker run -it -p 8080:8080 --name weather weatherapp"
  },
  "author": "Jeff Goes",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.8.1",
    "enzyme": "^3.11.0",
    "jest": "^26.6.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-geocode": "^0.2.2",
    "react-test-renderer": "^17.0.1",
    "regenerator-runtime": "^0.13.7",
    "styled-components": "^5.2.1",
    "weather-icons-react": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.10.4",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.4",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^4.2.2"
  }
}
