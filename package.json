    },
    "homepage": "https://github.com/douglasthmz/challenge-charlie#readme",
    "dependencies": {
        "axios": "^0.21.0",
        "classnames": "^2.2.6",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-loading": "^2.0.3"
    },
    "devDependencies": {
        "@babel/core": "^7.12.7",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-proposal-export-default-from": "^7.12.1",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.12.7",
        "@babel/preset-react": "^7.12.7",
