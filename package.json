{
    "name": "desafio-charlie-hu",
    "version": "1.0.0",
    "author": "Leonardo Gomes <lgdevlop@gmail.com>",
    "description": "Resolução do desafio front-end do Hotel Urbano",
    "repository": "git@github.com:lgdevlop/challenge-charlie.git",
    "keywords": [],
    "license": "MIT",
    "main": "index.js",
    "config": {
        "imageRepo": "lgdevlop/desafio-charlie",
        "imageName": "desafio_charlie_hu_app",
        "imagePort": "8080"
    },
    "scripts": {
        "start": "node src/server/index.js",
        "build": "webpack --config=config/webpack.pro.js",
        "dev": "webpack-dev-server --config=config/webpack.dev.js --progress --colors --inline --hot",
        "server": "node src/server/index.js",
        "debug": "node --debug-brk --inspect ./node_modules/jest/bin/jest -i",
        "test": "./node_modules/.bin/jest --env=jsdom",
        "predocker:build": "npm run build",
        "docker:build": "cross-conf-env docker image build . -t $npm_package_config_imageRepo:$npm_package_version",
        "postdocker:build": "npm run docker:rodando",
        "docker:rodando": "cross-conf-env docker run -e NODE_ENV=local --name $npm_package_config_imageName -d -p $npm_package_config_imagePort:8080 $npm_package_config_imageRepo:$npm_package_version",
        "postdocker:rodando": "echo Tentando executar no Windows && timeout 2 && start http://localhost:%npm_package_config_imagePort%"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-preset-env": "^1.7.0",
        "body-parser": "^1.18.3",
        "cors": "^2.8.4",
        "cross-conf-env": "^1.1.2",
        "css-loader": "^0.28.11",
        "cssnano": "^3.10.0",
        "express": "^4.16.3",
        "extract-loader": "^2.0.1",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^1.1.11",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^23.4.1",
        "less": "^3.7.1",
        "less-loader": "^4.1.0",
        "node-sass": "^4.9.2",
        "npm-run-all": "^4.1.3",
        "optimize-css-assets-webpack-plugin": "^4.0.3",
        "request": "^2.87.0",
        "sass-loader": "^7.0.3",
        "style-loader": "^0.21.0",
        "uglifyjs-webpack-plugin": "^1.2.7",
        "webpack": "^4.16.1",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.4",
        "whatwg-fetch": "^2.0.4"
    },
    "dependencies": {}
}
